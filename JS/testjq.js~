alert($('input').length);
tbox=[],pwd=[],fileupload=[],radbutton=[],chkbox=[],email=[],selbox=[];
for(i=0;i<$('input').length;i++)
{
if($('input')[i].type=="text")
tbox.push(i);
if($('input')[i].type=="password")
pwd.push(i);
if($('input')[i].type=="file")
fileupload.push(i);
if($('input')[i].type=="radio")
radbutton.push(i);
if($('input')[i].type=="checkbox")
chkbox.push(i);
if($('input')[i].type=="email")
email.push(i);
}
/*
for(i=0;i<$('select').length;i++)
{
}
*/
alert(tbox.length);
for(i=0;i<tbox.length;i++)
alert($('input')[tbox[i]].id);
alert(pwd.length);
for(i=0;i<pwd.length;i++)
alert($('input')[pwd[i]].id);
alert(fileupload.length);
for(i=0;i<fileupload.length;i++)
alert($('input')[fileupload[i]].id);
alert(chkbox.length);
for(i=0;i<chkbox.length;i++)
alert($('input')[chkbox[i]].id);
alert(radbutton.length);
for(i=0;i<radbutton.length;i++)
alert($('input')[radbutton[i]].id);
alert(email.length);
for(i=0;i<email.length;i++)
alert($('input')[email[i]].id);
